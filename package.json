{
  "name": "vite-template",
  "private": true,
  "description": "ViteJS + Typescript + Vitest",
  "author": {
    "name": "Harry Kodua",
    "email": "b.kodua@gmail.com",
    "url": "https://github.com/n1md7"
  },
  "license": "MIT",
  "main": "dist/main.js",
  "bugs": {
    "url": "https://github.com/n1md7/vite-JS/issues"
  },
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode=development",
    "build:github": "tsc && vite build --mode=github",
    "build:crazy": "tsc && vite build --mode=crazy",
    "build:itch": "tsc && vite build --mode=itch",
    "build:poki": "tsc && vite build --mode=poki",
    "test": "vitest --mode=test --no-watch",
    "preview": "vite preview",
    "format": "npx lint-staged",
    "husky": "husky install"
  },
  "dependencies": {
    "solid-js": "^1.8.16",
    "tiny-emitter": "^2.1.0",
    "tweakpane": "^4.0.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@tweakpane/core": "^2.0.3",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "sass": "^1.75.0",
    "solid-devtools": "^0.30.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-glsl": "^1.3.0",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^1.5.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "engineStrict": true
}
